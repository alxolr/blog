<div class="container">
  <div class="articles-container">
    <for(article in input.articles)>
      <article-card article=article/>
    </for>
  </div>
  <paginate paginate=input.paginate/>
</div>
style {
  .articles-container {
    display: flex;
    justify-content: space-around;
    flex-direction: row;
    flex-wrap: wrap;
  }
}
